{
  "name": "picktogram-server",
  "type": "commonjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "postbuild": "npx nestia swagger",
    "docker-compose": "docker-compose up",
    "docker-compose:build": "docker-compose -f docker-compose.local.yml up --build",
    "docker:build": "docker build -t picktogram/server:latest ./",
    "docker:start:local": "docker run -p 3000:3000 --env-file=.env picktogram/server:latest",
    "build": "nest build",
    "build:api": "tsc -p tsconfig.api.json",
    "build:tsc": "tsc -p tsconfig.build.json",
    "start:local": "cross-env NODE_ENV=LOCAL nest start --watch",
    "start:dev": "cross-env NODE_ENV=DEVELOPMENT nest start --watch",
    "start:prod": "node dist/main",
    "start:pm2": "cross-env NODE_ENV=PRODUCTION pm2 start",
    "pm2:reload": "npm run build && pm2 reload app",
    "test": "jest",
    "test:watch:legacy": "cross-env NODE_ENV=TEST jest --watch --detectOpenHandles",
    "test:e2e:watch": "cross-env NODE_ENV=TEST node --watch --require ts-node/register --test src/test/e2e/articles.spec.ts",
    "test:cov": "cross-env NODE_ENV=TEST jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "schema:drop:local": "cross-env NODE_ENV=LOCAL ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop -d ./data-source.ts",
    "schema:sync:local": "cross-env NODE_ENV=LOCAL ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./data-source.ts",
    "schema:sync:dev": "cross-env NODE_ENV=DEVELOPMENT ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./data-source.ts",
    "schema:sync:test": "cross-env NODE_ENV=TEST ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./data-source.ts",
    "schema:drop:test": "cross-env NODE_ENV=TEST ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop -d ./data-source.ts",
    "prepare": "ts-patch install && ts-patch install && ts-patch install && ts-patch install && ts-patch install && ts-patch install && ts-patch install",
    "sdk": "npx nestia sdk",
    "sdk:fix": "node ../nestia sdk",
    "swagger": "npx nestia swagger",
    "publish": "cd packages/api/lib && npm publish"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.332.0",
    "@nestia/core": "^1.2.2",
    "@nestia/fetcher": "^1.2.2",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/passport-google-oauth20": "^2.0.11",
    "aws-sdk": "^2.1377.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.1",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.1",
    "faucet": "^0.0.4",
    "multer": "^1.4.3",
    "multer-s3": "^3.0.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.16",
    "typia": "^3.8.6"
  },
  "devDependencies": {
    "@nestia/sdk": "^1.2.6",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^20.1.3",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-kakao": "^1.0.0",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "inquirer": "^9.2.2",
    "nestia": "^4.2.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.9.5"
  },
  "types": "src/types/index.d.ts",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "allowJs": false
  }
}
